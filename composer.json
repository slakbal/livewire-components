{
    "name": "slakbal/tall-components",
    "description": "Simple TALL Components.",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Slakbal",
            "email": "test@slakbal.com",
            "homepage": "https://slakbal.com"
        }
    ],
    "homepage": "https://github.com/slakbal/tall-components",
    "keywords": [
        "Laravel",
        "Tall Components",
        "AlpineJs",
        "Livewire"
    ],
    "require": {
        "illuminate/support": "~8|~9",
        "blade-ui-kit/blade-icons": "*",
        "davidhsianturi/blade-bootstrap-icons": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0",
        "livewire/livewire": "*",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "pestphp/pest-plugin-livewire": "^1.0",
        "orchestra/canvas": "^6.1",
        "nunomaduro/collision": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Slakbal\\TallComponents\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Slakbal\\TallComponents\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slakbal\\TallComponents\\TallComponentsServiceProvider"
            ],
            "aliases": {
                "TallComponents": "Slakbal\\TallComponents\\Facades\\TallComponents"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}